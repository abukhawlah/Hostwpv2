{
  "buildCommand": "cd hostwp-website && npm install && npm run build",
  "outputDirectory": "hostwp-website/dist",
  "installCommand": "cd hostwp-website && npm install",
  "framework": "vite",
  "functions": {
    "hostwp-website/api/upmind-proxy.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
} 